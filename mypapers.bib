  date = {2011-07},
  pages = {2400-2414},
  author = {GóMez-Aparicio, Lorena and GarcíA-ValdéS, RaúL and RuíZ-Benito, Paloma and Zavala, Miguel A.},
  file = {/Users/mariehbrice/Zotero/storage/Z8TITL4E/GóMez-Aparicio et al. - 2011 - Disentangling the relative importance of climate, .pdf}
