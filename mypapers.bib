  author = {Madrigal-González, Jaime and Andivia, Enrique and Zavala, Miguel A. and Stoffel, Markus and Calatayud, Joaquín and Sánchez-Salguero, Raúl and Ballesteros-Cánovas, Juan},
  file = {/Users/mariehbrice/Zotero/storage/I2NV7K5S/Madrigal-González et al. - 2018 - Disentangling the relative role of climate change .pdf}
