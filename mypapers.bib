  pages = {607-620},
  author = {Roach, Katherine A. and Thorp, James H. and Delong, Michael D.},
  file = {/Users/mariehbrice/Zotero/storage/TYUMQAX3/Roach et al. - 2009 - Influence of lateral gradients of hydrologic conne.pdf}
