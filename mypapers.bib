@article{selwood_effects_2015,
  langid = {english},
  title = {The Effects of Climate Change and Land-Use Change on Demographic Rates and Population Viability: {{Effects}} of Climate and Land-Use on Demography},
  volume = {90},
  issn = {14647931},
  url = {http://doi.wiley.com/10.1111/brv.12136},
  doi = {10.1111/brv.12136},
  shorttitle = {The Effects of Climate Change and Land-Use Change on Demographic Rates and Population Viability},
  abstract = {Understanding the processes that lead to species extinctions is vital for lessening pressures on biodiversity. While species diversity, presence and abundance are most commonly used to measure the effects of human pressures, demographic responses give a more proximal indication of how pressures affect population viability and contribute to extinction risk. We reviewed how demographic rates are affected by the major anthropogenic pressures, changed landscape condition caused by human land use, and climate change. We synthesized the results of 147 empirical studies to compare the relative effect size of climate and landscape condition on birth, death, immigration and emigration rates in plant and animal populations. While changed landscape condition is recognized as the major driver of species declines and losses worldwide, we found that, on average, climate variables had equally strong effects on demographic rates in plant and animal populations. This is signiﬁcant given that the pressures of climate change will continue to intensify in coming decades. The effects of climate change on some populations may be underestimated because changes in climate conditions during critical windows of species life cycles may have disproportionate effects on demographic rates. The combined pressures of land-use change and climate change may result in species declines and extinctions occurring faster than otherwise predicted, particularly if their effects are multiplicative.},
  number = {3},
  journaltitle = {Biological Reviews},
  urldate = {2018-04-10},
  date = {2015-08},
  pages = {837-853},
  author = {Selwood, Katherine E. and McGeoch, Melodie A. and Mac Nally, Ralph},
  file = {/Users/mariehbrice/Zotero/storage/9GHQKNFB/Selwood et al. - 2015 - The effects of climate change and land-use change .pdf}
}

